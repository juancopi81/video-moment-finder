# Qdrant Cloud GitHub Login
QDRANT_URL=https://xxx-xxx.gcp.cloud.qdrant.io
QDRANT_API_KEY=api-key

# Cloudflare R2 GitHub Login
R2_ENDPOINT_URL=https://xxx.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your-access-key
R2_SECRET_ACCESS_KEY=your-secret-key
R2_BUCKET_NAME=video-thumbnails
R2_PUBLIC_URL=https://your-cdn.example.com  # optional, for public thumbnail URL
R2_UPLOAD_WORKERS=16  # parallel thumbnail uploads

# Supabase (PostgreSQL database)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SECRET_KEY=your-secret-key  # server-only; never expose in browser
